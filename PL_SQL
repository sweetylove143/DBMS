DELIMITER $$

CREATE PROCEDURE FindMaxNumber(IN num1 INT, IN num2 INT, IN num3 INT, OUT max_num INT)
BEGIN
    -- Find the maximum of three numbers
    IF num1 >= num2 AND num1 >= num3 THEN
        SET max_num = num1;
    ELSEIF num2 >= num1 AND num2 >= num3 THEN
        SET max_num = num2;
    ELSE
        SET max_num = num3;
    END IF;
END$$

DELIMITER ;
